{
  "name": "docker_web_app",
  "version": "1.0.0",
  "description": "Node.js on Docker",
  "author": "Douglas dos Santos <douglasdossantos22@gmail.com>",
  "main": "./app/index.ts",
  "scripts": {
    "start": "npm run prisma-db && npx ts-node --project app/tsconfig.json && nodemon ./app/index.ts ",
    "prisma-db": "npx prisma db push --preview-feature && npx prisma generate && npx prisma db seed --preview-feature ",
    "dev": "npm start --watch"
  },
  "nodemonConfig": {
    "delay": "3"
  },
  "prisma": {
    "schema": "./app/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@prisma/client": "^2.22.0",
    "@types/express": "4.11.1",
    "@types/node": "12.12.2",
    "express": "4.17.1",
    "node-ts": "5.1.1",
    "nodemon": "^2.0.7",
    "npm": "^6.14.8",
    "prisma": "^2.22.0",
    "ts-lint": "4.5.1",
    "ts-node-dev": "1.1.6",
    "typescript": "3.4.3"
  }
}
